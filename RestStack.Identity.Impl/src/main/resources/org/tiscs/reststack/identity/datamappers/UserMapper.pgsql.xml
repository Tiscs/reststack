<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.tiscs.reststack.identity.datamappers.UserMapper">
    <select id="findAll" resultType="org.tiscs.reststack.identity.datamodels.User">
        select
            "id",
            "createTime",
            "lastModified",
            "deleted",
            "activated",
            "name",
            "gender",
            "phoneNumber",
            "phoneNumberVerified"
        from "Users"
    </select>
</mapper>